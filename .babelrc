{
  "sourceType": "unambiguous",
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "ie >= 9",
          "last 2 iOS major versions",
          "last 2 Firefox major versions",
          "last 2 Safari major versions",
          "last 2 Chrome major versions"
        ]
      }
    }]
  ],
  "options": {
    "plugins": ["@babel/plugin-proposal-optional-chaining"]
  }
}
