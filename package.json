{
  "name": "@holidayextras/static-site-generator",
  "version": "9.1.0",
  "description": "Holiday Extras Static Site Generator in metalsmith / react",
  "repository": {
    "type": "git",
    "url": "https://github.com/holidayextras/static-site-generator.git"
  },
  "author": "Richard Stevens <richard.stevens@holidayextras.com",
  "license": "MIT",
  "main": "lib/index.js",
  "watch": {
    "trans": "src"
  },
  "scripts": {
    "build": "npm run trans",
    "prepublishOnly": "npm run build",
    "trans": "node_modules/.bin/babel src/ -d lib/",
    "lint": "standard src/**/* src/* --fix",
    "ci": "npm run lint"
  },
  "standard": {
    "ignore": [
      "src/metalsmith-prismic.js"
    ]
  },
  "dependencies": {
    "async": "^1.5.2",
    "lodash": "^4.17.21",
    "meta-marked": "^0.4.0",
    "metalsmith": "^2.3.0",
    "metalsmith-assets-improved": "^1.0.0",
    "metalsmith-markdown": "^1.1.0",
    "metalsmith-react-tpl": "github:holidayextras/metalsmith-react-tpl#v4.0.0",
    "mkdirp": "^0.5.1",
    "prismic.io": "^3.1.3",
    "rimraf": "^2.2.8",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "debug": "^2.2.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "^5.0.6",
    "redux": "^4.2.0",
    "standard": "^16.0.4",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.0"
  }
}
