    "metalsmith-react-tpl": "^3.0.0",
    "mkdirp": "^0.5.1",
    "prismic.io": "^3.1.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "^7.2.0",
