    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/cli": "7.24.8",
    "@babel/core": "7.24.9",
    "@babel/preset-env": "7.25.0",
    "@babel/preset-react": "7.24.7",
    "debug": "^2.2.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "^5.0.6",
