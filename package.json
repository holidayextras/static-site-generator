    "metalsmith": "^2.3.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-react-tpl": "^2.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.11.1",
    "prismic.io": "^3.1.3",
    "publish": "^0.5.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
