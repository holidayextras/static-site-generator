    "fs": "0.0.2",
    "jsonp": "^0.2.0",
    "meta-marked": "^0.4.0",
    "metalsmith": "^1.3.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-react-tpl": "^0.2.3",
    "moment": "^2.11.1",
    "path": "^0.12.7",
    "prismic.io": "^1.4.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "grunt": "^0.4.5",
